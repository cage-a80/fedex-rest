{
    "name": "cage-a80/fedex-rest",
    "description": "Laravel FedEx REST API wrapper",
    "type": "library",
    "license": "MIT",
    "keywords": [ "shipping", "fedex", "API", "REST" ],
    "authors": [
        {
            "name": "Eugene Bourne",
            "email": "bourne.eugene@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^8|^9|^10|^11",
        "guzzlehttp/guzzle": "^7.2",
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": { "CageA80\\FedEx\\": "src/FedEx" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CageA80\\FedEx\\ServiceProvider"
            ],
            "aliases": {
                "FedEx": "CageA80\\FedEx\\Facades\\FedExFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
